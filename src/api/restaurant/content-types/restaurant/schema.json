{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "singularName": "restaurant",
    "pluralName": "restaurants",
    "displayName": "Restaurant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "restaurant"
    },
    "table_bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::table-booking.table-booking",
      "mappedBy": "restaurant"
    },
    "location": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "contact_number": {
      "type": "string"
    },
    "rating": {
      "type": "decimal"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "menus": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::menu.menu",
      "mappedBy": "restaurant"
    },
    "favourites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "menu_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::menu-item.menu-item",
      "mappedBy": "restaurant"
    },
    "favourite": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::favourite.favourite",
      "inversedBy": "restaurants"
    }
  }
}
